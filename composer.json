{
    "version": "1.1.0",
    "name": "stepanenko3/nova-markdown",
    "description": "Nova Markdown advanced field",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.0",
        "stepanenko3/nova-filemanager": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://laravel-nova.site"
        },
        {
            "type": "path",
            "url": "../nova-filemanager"
        }
    ],
    "autoload": {
        "psr-4": {
            "Stepanenko3\\NovaMarkdown\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stepanenko3\\NovaMarkdown\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
